<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>API_URL</key>
	<string>${API_URL}</string>
	<key>BASE_DOMAIN</key>
	<string>${BASE_DOMAIN}</string>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>${app_bundle}</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>${app_name}</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>${app_version_name}</string>
	<key>CFBundleVersion</key>
	<string>${app_version_code}</string>
	<key>CHAIN_URL</key>
	<string>${CHAIN_URL}</string>
	<key>INCLUDE_WALLET</key>
	<string>${INCLUDE_WALLET}</string>
	<key>KYC_URL</key>
	<string>${KYC_URL}</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
		<key>NSAllowsLocalNetworking</key>
		<true/>
	</dict>
	<key>NSCameraUsageDescription</key>
	<string>${app_name} needs access to your camera to take photos for your post or profile.</string>
	<key>NSFaceIDUsageDescription</key>
	<string>We use Face ID to authentiate your account.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>${app_name} needs access to your photo library to upload photos for your post or profile.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>${app_name} needs access to your photo library to store photos.</string>
	<key>Notificaton Center URL</key>
	<string>${NOTIFICATION_URL}</string>
	<key>SHOW_REWARD</key>
	<string>${SHOW_REWARD}</string>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
	</array>
	<key>UIStatusBarStyle</key>
	<string>UIStatusBarStyleDarkContent</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UIUserInterfaceStyle</key>
	<string>Light</string>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<true/>
	<key>URL_PROTOCOL</key>
	<string>${URL_PROTOCOL}</string>
</dict>
</plist>
